{
  "name": "rc-dock",
  "version": "3.3.0+agg.14",
  "description": "dock layout for react component",
  "repository": {
    "type": "git",
    "url": "https://github.com/slavamuravey/rc-dock.git"
  },
  "keywords": [
    "react",
    "layout",
    "dock",
    "panel",
    "tabs",
    "drag-and-drop",
    "float",
    "window",
    "drag",
    "drop",
    "reactjs",
    "react-dom"
  ],
  "author": {
    "name": "Rick Zhou",
    "email": "rinick@gmail.com"
  },
  "maintainers": [
    {
      "name": "Viacheslav Muravyev",
