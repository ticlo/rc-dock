  "types": "lib/index.d.ts",
  "license": "Apache-2.0",
  "dependencies": {
    "classnames": "^2.5.1",
    "lodash": "^4.17.21",
    "rc-dropdown": "3.2.2",
    "rc-menu": "~9.8.4",
    "rc-new-window": "^0.1.13",
    "rc-tabs": "11.14.2",
    "react": "^18.3.1",
    "react-dnd": "^14.0.4",
    "react-dnd-html5-backend": "^14.0.2",
    "react-dom": "^18.3.1",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-proposal-export-default-from": "^7.16.7",
    "@babel/preset-env": "^7.16.8",
    "@types/classnames": "^2.3.1",
    "@types/hoist-non-react-statics": "^3.3.1",
    "@types/lodash": "^4.14.178",
    "@types/node": "^17.0.9",
    "@types/prismjs": "^1.16.6",
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@types/shelljs": "^0.8.11",
    "@types/uuid": "^8.3.3",
    "deasync": "0.1.29",
    "node-gyp": "^10.1.0",
    "parcel": "1.12.4",
    "prismjs": "^1.26.0",
    "sass": "^1.77.8",
    "shelljs": "^0.8.4",
    "ts-node": "^10.4.0",
    "tslint": "^6.1.3",
    "tslint-react": "^5.0.0",
    "typedoc": "0.20.37",
    "typescript": "4.2.4"
  },
  "peerDependencies": {
    "react": ">=15.0.0",
    "react-dom": ">=15.0.0"
  },
  "resolutions": {
    "@babel/preset-env": "7.13.8"
  },
  "scripts": {
    "example": "parcel example/*.html --open --out-dir temp --no-source-maps --no-hmr",
    "build-style": "sass --no-source-map style/index-light.scss dist/rc-dock.css & sass --no-source-map style/index-dark.scss dist/rc-dock-dark.css",
    "build-doc": "typedoc",
    "build-www": "ts-node tool/build-www",
    "build-es": "tsc --module es2020 --outDir ./es",
    "build-lib": "tsc --outDir ./lib",
    "build": "npx yarn run build-style && npx yarn run build-es && npx yarn run build-lib"
  }
